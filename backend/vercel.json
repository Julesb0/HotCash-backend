{
  "version": 2,
  "functions": {
    "backend/target/entrepreneur-platform-0.0.1-SNAPSHOT.jar": {
      "runtime": "java17"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/target/entrepreneur-platform-0.0.1-SNAPSHOT.jar"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase-url",
    "SUPABASE_ANON_KEY": "@supabase-anon-key",
    "SUPABASE_SERVICE_ROLE_KEY": "@supabase-service-role-key",
    "JWT_SECRET": "@jwt-secret"
  }
}